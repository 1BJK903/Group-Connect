{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "148a66bb-e83d-425a-927d-09f4299a9274",
  "packageName": "com.microsoft.teams.diconnect",
  "developer": {
    "name": "<<companyName>>",
    "websiteUrl": "<<websiteUrl>>",
    "privacyUrl": "<<privacyUrl>>",
    "termsOfUseUrl": "<<termsOfUseUrl>>"
  },
  "localizationInfo": {
    "defaultLanguageTag": "en"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Diversity & Inclusion",
    "full": "Diversity & Inclusion Connect App"
  },
  "description": {
    "short": "App is meant to help customers promote a culture of diversity and inclusion",
    "full": "App is meant to help customers promote a culture of diversity and inclusion in their company, directly from Microsoft Teams"
  },
  "accentColor": "#64A2CC",
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "personal",
        "team"
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "configurableTabs": [
    {
      "configurationUrl": "https://<<appDomain>>/ergChannelConfigTab",
      "canUpdateConfiguration": true,
      "scopes": [
        "team"
      ]
    }
  ],
  "staticTabs": [
    {
      "entityId": "Discover",
      "name": "Discover groups",
      "contentUrl": "https://<<appDomain>>/discover",
      "websiteUrl": "https://<<appDomain>>/discover",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<appDomain>>"
  ],
  "webApplicationInfo": {
    "id": "<<botId>>",
    "resource": "api://<<appDomain>>"
  }
}